{
  "search.exclude": {
    "**/.yarn": true,
  },
  "eslint.workingDirectories": [
    "src"
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.packageManager": "yarn",
  "git.mergeEditor": false,
  "svg.preview.background": "custom"
}
